{
    "description": "Generate a random number as integer or string",
    "label": "Random Number",
    "category": "Misc",
    "icon": {
        "name": "NumberIcon",
        "color": "Orange"
    },
    "options": [
        {
            "name": "min",
            "label": "Minimum",
            "info": "Enter the minimum value for the random number",
            "meta": {
                "type": "Number",
                "validations": {
                    "required": true
                }
            },
            "configuration": {
                "placeholder": "0"
            }
        },
        {
            "name": "max",
            "label": "Maximum",
            "info": "Enter the maximum value for the random number",
            "meta": {
                "type": "Number",
                "validations": {
                    "required": true
                }
            },
            "configuration": {
                "placeholder": "100"
            }
        },
        {
            "name": "returning",
            "label": "As",
            "info": "The name of the variable you want for the generated number",
            "meta": {
                "type": "Output",
                "validations": {
                    "required": true
                },
                "output": {
                    "anyOf": [{ "type": "Number" }, { "type": "Text" }]
                }
            },
            "configuration": {
                "placeholder": "random_number"
            }
